{
  "name": "boilerplate.react.spa",
  "description": "A boilerplate for creating a SPA in React.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the0neWhoKnocks/boilerplate.react.spa.git"
  },
  "homepage": "https://github.com/the0neWhoKnocks/boilerplate.react.spa#readme",
  "author": "Trevor Lemon",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "axios": "^0.17.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1"
  },
  "scripts": {
    "build": "cross-env PUBLIC_URL=\"/\" BABEL_ENV=\"production\" NODE_ENV=\"production\" node .bin/build.js",
    "start": "cross-env BABEL_ENV=\"development\" NODE_ENV=\"development\" node .bin/start.js",
    "start:build": "http-server ./build --gzip -o -p 8080",
    "test": "cross-env BABEL_ENV=\"test\" NODE_ENV=\"test\" jest --env=jsdom --config ./conf.jest.js",
    "test:watch": "npm run test -- --watchAll"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^1.0.1",
    "cross-env": "^5.1.1",
    "http-server": "^0.10.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  }
}
