{
  "name": "boilerplate.react.spa",
  "description": "A boilerplate for creating a SPA in React.",
  "version": "2.0.0",
  "config": {
    "SERVER_PORT": 3000
  },
  "scripts": {
    "build": "cross-env PUBLIC_URL=\"/\" BABEL_ENV=\"production\" NODE_ENV=\"production\" node .bin/build.js",
    "start": "http-server ./public --gzip -o -p $npm_package_config_SERVER_PORT",
    "start:dev": "cross-env BABEL_ENV=\"development\" NODE_ENV=\"development\" node .bin/start.js",
    "test": "eslint ./**/*.js && cross-env BABEL_ENV=\"test\" NODE_ENV=\"test\" jest --env=jsdom --config ./conf.jest.js",
    "test:watch": "npm run test -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the0neWhoKnocks/boilerplate.react.spa.git"
  },
  "homepage": "https://github.com/the0neWhoKnocks/boilerplate.react.spa#readme",
  "author": "Trevor Lemon",
  "license": "MIT",
  "dependencies": {
    "@noxx/webpack-tidy-plugin": "^1.0.2",
    "autoprefixer": "7.1.2",
    "axios": "^0.17.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "dotenv": "4.0.0",
    "eslint": "^4.18.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "extend": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "glamor": "^2.20.40",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "^3.11.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@noxx/eslint-config": "^1.0.5",
    "compression-webpack-plugin": "^1.0.1",
    "cross-env": "^5.1.1",
    "http-server": "^0.10.0"
  }
}
